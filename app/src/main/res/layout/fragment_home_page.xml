<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homepage11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEFE"
    tools:context=".HomePage">

    <!-- TODO: Update blank fragment layout -->


    <Button
        android:id="@+id/carrer1"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="65dp"
        android:layout_height="302dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="57dp"
        android:backgroundTint="#7BD1E8"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="carrer"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/help"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/help"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="63dp"
        android:layout_height="277dp"

        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="64dp"
        android:backgroundTint="#AEBFF1"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="help"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carrer1"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="411dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="125dp"
        android:layout_height="93dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="143dp"
        android:src="@drawable/pandaset"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/help"
        app:layout_constraintTop_toBottomOf="@+id/playES"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:text="BEST SCORE:"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/playHARD"
        app:layout_constraintEnd_toEndOf="@+id/playHARD"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/playHARD"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/playES"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="106dp"
        android:layout_height="106dp"
        android:backgroundTint="#7BD1E8"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="EASYmode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/playMID"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="146dp"
        android:layout_height="92dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="18dp"
        android:src="@drawable/hi"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.322"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/playHARD"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="104dp"
        android:layout_height="106dp"
        android:layout_marginTop="14dp"
        android:backgroundTint="#AEBFF1"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="HARDmode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/playMID"
        app:layout_constraintEnd_toEndOf="@+id/playMID"
        app:layout_constraintStart_toStartOf="@+id/playMID"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/playMID"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="106dp"
        android:layout_height="106dp"
        android:backgroundTint="#A2F1EF"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="MID mode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/playES"
        app:layout_constraintEnd_toEndOf="@+id/playES"
        app:layout_constraintStart_toStartOf="@+id/playES"
        app:layout_constraintTop_toBottomOf="@+id/playHARD"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>