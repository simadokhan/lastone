<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homepage11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEFE"
    tools:context=".HomePage">

    <!-- TODO: Update blank fragment layout -->


    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="393dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="124dp"
        android:layout_height="94dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="193dp"
        android:src="@drawable/hi"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bestscore"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bestscore"
        android:layout_width="264dp"
        android:layout_height="38dp"
        android:layout_marginStart="23dp"
        android:layout_marginBottom="20dp"
        android:text="BEST SCORE:"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="121dp"
        android:layout_height="94dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="138dp"
        android:src="@drawable/paaaaaaaaaaaaaaaaaa"
        app:layout_constraintBottom_toTopOf="@+id/playMID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/playHARD"
        app:layout_constraintTop_toBottomOf="@+id/bestscore"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/playES"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="126dp"
        android:layout_height="103dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="145dp"
        android:backgroundTint="#7BD1E8"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="EASY
        mode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/carrer1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/help"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/playHARD"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="125dp"
        android:layout_height="104dp"
        android:layout_marginStart="27dp"
        android:backgroundTint="#AEBFF1"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="@string/hard_mode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/playMID"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="123dp"
        android:layout_height="106dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="87dp"
        android:backgroundTint="#A2F1EF"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="MID mode"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/help"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="95dp"
        android:layout_height="60dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="15dp"
        android:src="@drawable/panda2bbb"
        app:layout_constraintBottom_toTopOf="@+id/playES"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/playMID"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="139dp"
        android:layout_height="92dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="4dp"
        android:src="@drawable/pandaface"
        app:layout_constraintBottom_toTopOf="@+id/help"
        app:layout_constraintEnd_toStartOf="@+id/playMID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/playHARD" />

    <Button
        android:id="@+id/help"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="154dp"
        android:layout_height="82dp"

        android:layout_marginStart="9dp"
        android:layout_marginEnd="114dp"
        android:backgroundTint="#AEBFF1"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="help"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/Top10"
        app:layout_constraintEnd_toStartOf="@+id/playES"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/carrer1"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="297dp"
        android:layout_height="81dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="53dp"
        android:backgroundTint="#7BD1E8"
        android:fontFamily="sans-serif-smallcaps"
        android:onClick="didaacc"
        android:text="carrer and setting "
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Top10"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Top10"
        style="@style/Widget.Material3.Button.TonalButton"
        android:layout_width="235dp"
        android:layout_height="79dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="157dp"
        android:backgroundTint="#A2F1EF"
        android:fontFamily="sans-serif-smallcaps"
        android:text="TOP Players "
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/carrer1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/help"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="101dp"
        android:layout_height="81dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="159dp"
        android:src="@drawable/pp"
        app:layout_constraintBottom_toTopOf="@+id/playHARD"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bestscore" />


</androidx.constraintlayout.widget.ConstraintLayout>