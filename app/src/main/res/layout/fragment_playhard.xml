<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Playhard">
    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:id="@+id/circle2h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginStart="35dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toStartOf="@+id/circle8h"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circle4h"
        tools:ignore="NotSibling" />

    <ImageView
        android:id="@+id/circle4h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="121dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toStartOf="@+id/circle9h"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timer"
        tools:ignore="NotSibling" />

    <ImageView
        android:id="@+id/circle1h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toStartOf="@+id/circle7h"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circle2h" />

    <ImageView
        android:id="@+id/circle8h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:src="@drawable/circle"
        app:layout_constraintBottom_toTopOf="@+id/circle7h"
        app:layout_constraintEnd_toStartOf="@+id/circle5h"
        app:layout_constraintStart_toEndOf="@+id/circle2h"
        app:layout_constraintTop_toBottomOf="@+id/circle9h" />

    <ImageView
        android:id="@+id/circle7h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toStartOf="@+id/circle6h"
        app:layout_constraintStart_toEndOf="@+id/circle1h"
        app:layout_constraintTop_toBottomOf="@+id/circle8h" />

    <ImageView
        android:id="@+id/circle9h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toStartOf="@+id/circle3h"
        app:layout_constraintStart_toEndOf="@+id/circle4h"
        app:layout_constraintTop_toBottomOf="@+id/HARDSCORE" />

    <ImageView
        android:id="@+id/circle3h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="37dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circle9h"
        app:layout_constraintTop_toBottomOf="@+id/HARDSCORE" />

    <ImageView
        android:id="@+id/circle6h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginEnd="37dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circle7h"
        app:layout_constraintTop_toBottomOf="@+id/circle5h" />

    <ImageView
        android:id="@+id/circle5h"
        android:layout_width="107dp"
        android:layout_height="117dp"
        android:layout_marginEnd="37dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circle8h"
        app:layout_constraintTop_toBottomOf="@+id/circle3h" />

    <TextView
        android:id="@+id/HARDSCORE"
        android:layout_width="126dp"
        android:layout_height="54dp"
        android:layout_marginStart="142dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="37dp"
        android:text="score"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/circle9h"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/exit"
        android:layout_width="190dp"
        android:layout_height="144dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="211dp"
        android:layout_marginBottom="17dp"
        android:backgroundTint="#FFFFFF"
        android:src="@drawable/project1234"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circle1h" />

    <TextView
        android:id="@+id/timer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="353dp"
        android:layout_marginBottom="121dp"
        android:text="00:00"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/circle4h"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>